<h1>Listing production_order_handlers</h1>

<table>
  <thead>
    <tr>
      <th>Nr</th>
      <th>Desc</th>
      <th>Remark</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @production_order_handlers.each do |production_order_handler| %>
      <tr>
        <td><%= production_order_handler.nr %></td>
        <td><%= production_order_handler.desc %></td>
        <td><%= production_order_handler.remark %></td>
        <td><%= link_to 'Show', production_order_handler %></td>
        <td><%= link_to 'Edit', edit_production_order_handler_path(production_order_handler) %></td>
        <td><%= link_to 'Destroy', production_order_handler, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Production order handler', new_production_order_handler_path %>
