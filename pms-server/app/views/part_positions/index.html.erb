<h1>Listing part_positions</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @part_positions.each do |part_position| %>
      <tr>
        <td><%= link_to 'Show', part_position %></td>
        <td><%= link_to 'Edit', edit_part_position_path(part_position) %></td>
        <td><%= link_to 'Destroy', part_position, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
