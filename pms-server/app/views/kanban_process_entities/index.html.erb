<h1>Listing kanban_process_entities</h1>

<table>
  <thead>
    <tr>
      <th>Kanban</th>
      <th>Processentity</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @kanban_process_entities.each do |kanban_process_entity| %>
      <tr>
        <td><%= kanban_process_entity.Kanban %></td>
        <td><%= kanban_process_entity.ProcessEntity %></td>
        <td><%= link_to 'Show', kanban_process_entity %></td>
        <td><%= link_to 'Edit', edit_kanban_process_entity_path(kanban_process_entity) %></td>
        <td><%= link_to 'Destroy', kanban_process_entity, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

